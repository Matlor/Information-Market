{
	"canisters": {
		"Prototype": {
			"main": "backend/src/main.mo",
			"type": "motoko",
			"declarations": {
				"output": "declarations/Prototype"
			}
		},
		"Frontend": {
			"frontend": {
				"entrypoint": "index.html"
			},
			"source": ["frontend/dist/"],
			"type": "assets",
			"declarations": {
				"output": "declarations/Frontend"
			}
		}
	},
	"networks": {
		"local": {
			"bind": "127.0.0.1:8000",
			"type": "ephemeral"
		},
		"ic": {
			"providers": ["https://mainnet.dfinity.network"],
			"type": "persistent"
		}
	},
	"version": 1
}