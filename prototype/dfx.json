{
	"canisters": {
        "playground": {
            "type": "assets",
            "source": ["canisters/playground/build"]
        },
		"graphql": {
            "type": "custom",
            "build": "cargo build --target wasm32-unknown-unknown --package graphql --release",
            "candid": "canisters/graphql/src/graphql.did",
            "wasm": "target/wasm32-unknown-unknown/release/graphql.wasm"
        },
        "querybuilder": {
            "type": "custom",
            "build": "cargo build --target wasm32-unknown-unknown --package querybuilder --release",
            "candid": "canisters/querybuilder/src/querybuilder.did",
            "wasm": "target/wasm32-unknown-unknown/release/querybuilder.wasm",
            "dependencies": ["graphql"]
        }
	},
    "defaults": {
        "replica": {
            "subnet_type": "system"
        }
    },
	"version": 1
}
